<div class="container">
  <h3 class="d-inline-block px-4 ml-5"><i class="fas fa-at"></i> 配送先編集</h3>
   <div id="error">
     <%= render 'layouts/error_messages', model: @address_date %>
   </div>
   <div class="me-4 mt-3">
     <%= form_with model: @address_date, url: address_path(@address_date), method: :patch do |f| %>
     <div class="form-group row">
         <%= f.label :郵便番号（ハイフンなし）,  class: "col-3" %>
         <%= f.text_field :postal_code, "placeholder" => "0000000" %>
     </div>
     <div class="form-group row">
       <%= f.label :住所, class: "col-3" %>
       <%= f.text_field :address, class: "col-5", "placeholder" => "大阪府大阪市中央区0-0-0" %>
     </div>
     <div class="form-group row">
       <%= f.label :宛名, class: "col-3" %>
       <%= f.text_field :name,  "placeholder" => "鈴木佳子" %>
       <%= f.submit "新規登録", class: "btn btn-success col-2 ml-auto mr-3" %>
     </div>
     <% end %>
   </div>
</div>