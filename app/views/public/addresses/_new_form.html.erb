<%= form_with model: address_date, class: "js-form", local: false do |f| %>
<div class="form-group row">
  <%= f.label :郵便番号（ハイフンなし）,  class: "col-3" %>
  <%= f.text_field :postal_code, "placeholder" => "0000000" %>
</div>
<div class="form-group row">
  <%= f.label :住所, class: "col-3" %>
  <%= f.text_field :address, class: "col-5", "placeholder" => "大阪府大阪市中央区0-0-0" %>
</div>
<div class="form-group row">
   <%= f.label :宛名, class: "col-3" %>
   <%= f.text_field :name,  "placeholder" => "鈴木佳子" %>
   <%= f.submit "新規登録", class: "btn btn-success col-2 ml-auto mr-3" %>
</div>
<% end %>