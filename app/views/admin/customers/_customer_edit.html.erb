<div class="form-group">
  <%= f.label :氏名, class: "col-2" %>
  <%= f.text_field :family_name, autofocus: true, placeholder: "山田", class: "mr-5" %>
  <%= f.text_field :first_name, placeholder: "太郎" %>
</div>
<div class="form-group">
  <%= f.label :フリガナ, class: "col-2" %>
  <%= f.text_field :family_name_kana, placeholder: "ヤマダ" class: "mr-5" %>
  <%= f.text_field :first_name_kana, placeholder: "タロウ" %>
</div>
<div class="form-group">
  <%= f.label :郵便番号(ハイフンなし), class: "col-2" %>
  <%= f.text_field :postal_code, placeholder: "0000000" %>
</div>
<div class="form-group">
  <%= f.label :住所, class: "col-2" %>
  <%= f.text_field :address, placeholder: "大阪府大阪市中央区0-0-0", class: "col-9" %>
</div>
<div class="form-group">
  <%= f.label :電話番号(ハイフンなし), class: "col-2" %>
  <%= f.text_field :telephone_number, placeholder: "00000000000" %>
</div>
<div class="form-group">
  <%= f.label :メールアドレス, class: "col-2" %>
  <%= f.email_field, :email, autocomplete: "email", placeholder: "sample@example.com" %>
</div>